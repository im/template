{
    "editor.formatOnSave": true,
    "eslint.format.enable": true,
    "eslint.enable": true,
    "eslint.validate": [
        "javascript",
        "typescript",
        "javascriptreact",
        "vue",
        "html"
    ],
    "eslint.options": {
        "overrideConfigFile": "./.eslintrc.js"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.alwaysShowStatus": true
}